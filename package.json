{
  "name": "@regions-of-indonesia/data",
  "version": "5.0.0-1",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./provinces": {
      "import": {
        "types": "./dist/provinces.d.ts",
        "default": "./dist/provinces.js"
      },
      "require": {
        "types": "./dist/provinces.d.ts",
        "default": "./dist/provinces.cjs"
      }
    },
    "./districts": {
      "import": {
        "types": "./dist/districts.d.ts",
        "default": "./dist/districts.js"
      },
      "require": {
        "types": "./dist/districts.d.ts",
        "default": "./dist/districts.cjs"
      }
    },
    "./subdistricts": {
      "import": {
        "types": "./dist/subdistricts.d.ts",
        "default": "./dist/subdistricts.js"
      },
      "require": {
        "types": "./dist/subdistricts.d.ts",
        "default": "./dist/subdistricts.cjs"
      }
    },
    "./villages": {
      "import": {
        "types": "./dist/villages.d.ts",
        "default": "./dist/villages.js"
      },
      "require": {
        "types": "./dist/villages.d.ts",
        "default": "./dist/villages.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "private": false,
  "repository": "https://github.com/regions-of-indonesia/data.git",
  "author": {
    "name": "flamrdevs",
    "url": "https://github.com/flamrdevs"
  },
  "license": "GPL-3.0",
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build": "vite build",
    "release": "pnpm test && pnpm build && release-it"
  },
  "devDependencies": {
    "@regions-of-indonesia/types": "1.0.0-0",
    "@regions-of-indonesia/utils": "1.0.0-3",
    "@types/prettier": "^2.7.2",
    "@vitest/ui": "^0.31.1",
    "fs-jetpack": "^5.1.0",
    "mysql2": "^3.3.2",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "table": "^6.8.1",
    "tsx": "^3.12.7",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1"
  }
}
