{
  "name": "@regions-of-indonesia/data",
  "version": "5.1.1",
  "type": "module",
  "main": "./main.cjs",
  "types": "./main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./main.d.ts",
        "default": "./main.js"
      },
      "require": {
        "types": "./main.d.ts",
        "default": "./main.cjs"
      }
    },
    "./province": {
      "import": {
        "types": "./dist/province.d.ts",
        "default": "./dist/province.js"
      },
      "require": {
        "types": "./dist/province.d.ts",
        "default": "./dist/province.cjs"
      }
    },
    "./district": {
      "import": {
        "types": "./dist/district.d.ts",
        "default": "./dist/district.js"
      },
      "require": {
        "types": "./dist/district.d.ts",
        "default": "./dist/district.cjs"
      }
    },
    "./subdistrict": {
      "import": {
        "types": "./dist/subdistrict.d.ts",
        "default": "./dist/subdistrict.js"
      },
      "require": {
        "types": "./dist/subdistrict.d.ts",
        "default": "./dist/subdistrict.cjs"
      }
    },
    "./village": {
      "import": {
        "types": "./dist/village.d.ts",
        "default": "./dist/village.js"
      },
      "require": {
        "types": "./dist/village.d.ts",
        "default": "./dist/village.cjs"
      }
    }
  },
  "files": [
    "dist",
    "main.cjs",
    "main.d.ts",
    "main.js",
    "province.d.ts",
    "district.d.ts",
    "subdistrict.d.ts",
    "village.d.ts"
  ],
  "private": false,
  "repository": "https://github.com/regions-of-indonesia/data.git",
  "author": {
    "name": "flamrdevs",
    "url": "https://github.com/flamrdevs"
  },
  "license": "GPL-3.0",
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build": "vite build",
    "release": "pnpm build && pnpm test && release-it"
  },
  "devDependencies": {
    "@regions-of-indonesia/types": "5.0.1",
    "@regions-of-indonesia/utils": "5.0.1",
    "@types/prettier": "^2.7.2",
    "@vitest/ui": "^0.31.1",
    "fs-jetpack": "^5.1.0",
    "mysql2": "^3.3.2",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "table": "^6.8.1",
    "tsx": "^3.12.7",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1"
  }
}
