{
  "name": "@regions-of-indonesia/data",
  "version": "0.4.0",
  "description": "Regions of Indonesia data",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "ts-node-esm --project scripts/tsconfig.json scripts/index.ts",
    "build": "rimraf dist && microbundle src/index.ts --format esm,cjs --sourcemap false",
    "release": "release-it"
  },
  "repository": "https://github.com/regions-of-indonesia/data.git",
  "author": "flamrdevs <flamrdevs@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "devDependencies": {
    "fs-jetpack": "^4.3.1",
    "microbundle": "^0.15.1",
    "mysql2": "^2.3.3",
    "release-it": "^15.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
