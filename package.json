{
  "name": "@regions-of-indonesia/data",
  "version": "3.1.0",
  "description": "Regions of Indonesia data",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.modern.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "rimraf ./src && ts-node-esm --project scripts/tsconfig.json scripts/index.ts",
    "generate:rewrite": "rimraf ./src &&set REWRITE=true&& ts-node-esm --project scripts/tsconfig.json scripts/index.ts",
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "build": "microbundle src/index.ts --format modern,cjs --sourcemap false",
    "prerelease": "yarn run test && yarn run build",
    "release": "release-it --only-version"
  },
  "repository": "https://github.com/regions-of-indonesia/data.git",
  "author": "flamrdevs <flamrdevs@gmail.com>",
  "homepage": "https://regions-of-indonesia.netlify.app",
  "license": "GPL-3.0",
  "private": false,
  "keywords": [
    "api",
    "library",
    "open-source",
    "javascript",
    "typescript",
    "indonesia",
    "region",
    "daerah",
    "wilayah",
    "wilayah-indonesia",
    "regions-of-indonesia"
  ],
  "devDependencies": {
    "@types/prettier": "^2.7.2",
    "@vitest/ui": "^0.29.5",
    "fs-jetpack": "^5.1.0",
    "microbundle": "^0.15.1",
    "mysql2": "^3.2.0",
    "prettier": "^2.8.5",
    "release-it": "^15.9.0",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vitest": "^0.29.5"
  }
}
